namespace cpu_monitor.msg;

table ThreadInfo {
  name: string;
  id: uint64;
  usage: float32;
  timestamps: uint64;
}

table MemInfo {
  peak: uint64;
  size: uint64;
  hwm: uint64;
  rss: uint64;
  timestamps: uint64;
}

table ProcessInfo {
  id: uint64;
  name: string;
  thread_infos: [ThreadInfo];
  mem_info: MemInfo;
}

table ProcessMsg {
  infos: [ProcessInfo];
  timestamps: uint64;
}

root_type ProcessMsg;
